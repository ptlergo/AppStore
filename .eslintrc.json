{
	"env": {
		"node": true,
		"mocha": true
	},
	"extends": "airbnb",
	"plugins": [
		"react"
	],
	"globals": {
		"describe": true,
		"it": true,
		"null": true
	},
	"rules": {
	"global-require": 0,
	"prefer-template": 0,
	"new-cap": 0,
	"no-console": 0,
	"no-unused-vars": 0,
	"no-loop-func": 0,
	"no-unused-vars": 0,
	"import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
	}
}
